{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://bcrs.gov.bc.ca/.well_known/schemas/account_fee",
  "type": "object",
  "title": "Account Fee",
  "required": [
  ],
  "properties": {
    "applyFilingFees": {
      "$id": "#/properties/applyFilingFees",
      "type": "boolean",
      "title": "Apply filing fees",
      "default": true,
      "pattern": "^(.*)$"
    },
    "serviceFeeCode": {
      "$id": "#/properties/serviceFeeCode",
      "type": "string",
      "title": "Service Fee Code",
      "default": "",
      "examples": [
        "TRF01"
      ],
      "pattern": "^(.*)$"
    },
    "product": {
      "$id": "#/properties/product",
      "type": "string",
      "title": ":Product",
      "default": "",
      "examples": [
        "BUSINESS", ""
      ],
      "pattern": "^(.*)$"
    }
  }
}